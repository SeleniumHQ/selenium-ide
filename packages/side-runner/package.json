{
  "name": "selenium-side-runner",
  "version": "4.0.0-alpha.59",
  "private": false,
  "description": "Run Selenium IDE projects in cli",
  "repository": "https://github.com/SeleniumHQ/selenium-ide",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist tsconfig.tsbuildinfo node_modules",
    "start": "node dist/bin.js",
    "watch": "tsc --watch"
  },
  "bin": {
    "selenium-side-runner": "dist/bin.js"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "selenium",
    "command",
    "line"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@seleniumhq/side-model": "^4.0.0-alpha.4",
    "@seleniumhq/side-runtime": "^4.0.0-alpha.30",
    "commander": "^11.0.0",
    "glob": "^10.3.1",
    "jest": "^29.6.0",
    "jest-cli": "^29.6.0",
    "jest-each": "^29.6.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "resolve-bin": "^1.0.1",
    "rimraf": "^5.0.1",
    "semver": "^7.5.3",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.199",
    "@types/resolve-bin": "^0.4.1",
    "@types/semver": "^7.5.0"
  }
}
